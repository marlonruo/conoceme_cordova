<?xml version='1.0' encoding='utf-8'?>
<widget id="com.conoceme_mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ConoceME!</name>
    <description>
        Con ConóceME! vas a conocer gente para hacer nuevos amigos de América Latina.
    </description>
    <author email="marlonruo@gmail.com" href="conoceme.vip">
        Nospun
    </author>
    <content src="index.html" />
